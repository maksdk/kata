{
  "plugins": [
    "import"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": [
    "plugin:import/errors",
    "plugin:import/warnings",
    "airbnb-base"
  ],
  "rules": {
    "no-console": 0,
    "import/extensions": 0,
    "class-methods-use-this": 0,
    "no-trailing-spaces": 0,
    "comma-dangle": 0,
    "eol-last": 0,
    "import/prefer-default-export": 0
  }
}